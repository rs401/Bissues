<?xml version="1.0" encoding="utf-8" ?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat code coverage">
        <Configuration>
          <ExcludeByFile>
            **/ViewModels/*.cs,
            **/Areas/Identity/**/*.cs,
            **/Areas/Identity/**/*.cshtml,
            **/Data/Migrations/**/*.cs,
            **/Templates/**/*.cshtml,
          </ExcludeByFile>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>